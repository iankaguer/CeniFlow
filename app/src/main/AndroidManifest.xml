<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aztechlabs.ceniflow">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_logoceni"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logoceni"
        android:supportsRtl="true"
        android:theme="@style/Theme.CeniFlow">
        <activity android:name=".Resultats" android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity android:name=".Candidats" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".Partis" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity android:name=".BureauVote" android:theme="@style/Theme.AppCompat.NoActionBar"/>
        <activity
            android:name=".Splash"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
    </application>

</manifest>